{"ast":null,"code":"import ContentBase from \"../components/ContentBase.vue\";\nimport UserProfileInfo from \"../components/UserProfileInfo.vue\";\nimport UserProfilePosts from \"../components/UserProfilePosts.vue\";\nimport UserProfileWrite from \"@/components/UserProfileWrite.vue\";\nimport { reactive } from \"vue\";\nexport default {\n  name: \"UserProfileView\",\n  components: {\n    ContentBase,\n    UserProfileInfo,\n    UserProfilePosts,\n    UserProfileWrite\n  },\n  setup() {\n    const user = reactive({\n      id: 1,\n      userName: \"liule\",\n      lastName: \"Liu\",\n      firstName: \"Le\",\n      followerCount: 0,\n      is_followed: false\n    });\n    const posts = reactive({\n      count: 3,\n      posts: [{\n        id: 1,\n        userId: 1,\n        content: \"今天上了web课真开心\"\n      }, {\n        id: 2,\n        userId: 1,\n        content: \"今天上了算法课真开心\"\n      }, {\n        id: 3,\n        userId: 1,\n        content: \"今天上了acwing真开心\"\n      }]\n    });\n    const follow = () => {\n      if (user.is_followed) return;\n      user.is_followed = true;\n      user.followerCount++;\n    };\n    const unfollow = () => {\n      if (!user.is_followed) return;\n      user.is_followed = false;\n      user.followerCount--;\n    };\n    return {\n      user,\n      follow,\n      unfollow,\n      posts\n    };\n  }\n};","map":{"version":3,"names":["ContentBase","UserProfileInfo","UserProfilePosts","UserProfileWrite","reactive","name","components","setup","user","id","userName","lastName","firstName","followerCount","is_followed","posts","count","userId","content","follow","unfollow"],"sources":["/Users/le/myspace/src/views/UserProfileView.vue"],"sourcesContent":["<template>\n  <ContentBase>\n    <div class=\"row\">\n      <div class=\"col-3\">\n        <UserProfileInfo @follow=\"follow\" @unfollow=\"unfollow\" :user=\"user\" />\n      </div>\n      <div class=\"col-9\">\n        <UserProfilePosts :posts=\"posts\" />\n      </div>\n    </div>\n  </ContentBase>\n</template>\n\n<script>\nimport ContentBase from \"../components/ContentBase.vue\";\nimport UserProfileInfo from \"../components/UserProfileInfo.vue\";\nimport UserProfilePosts from \"../components/UserProfilePosts.vue\";\nimport UserProfileWrite from \"@/components/UserProfileWrite.vue\";\nimport { reactive } from \"vue\";\n\nexport default {\n  name: \"UserProfileView\",\n  components: {\n    ContentBase,\n    UserProfileInfo,\n    UserProfilePosts,\n    UserProfileWrite,\n  },\n\n  setup() {\n    const user = reactive({\n      id: 1,\n      userName: \"liule\",\n      lastName: \"Liu\",\n      firstName: \"Le\",\n      followerCount: 0,\n      is_followed: false,\n    });\n\n    const posts = reactive({\n      count: 3,\n      posts: [\n        {\n          id: 1,\n          userId: 1,\n          content: \"今天上了web课真开心\",\n        },\n        {\n          id: 2,\n          userId: 1,\n          content: \"今天上了算法课真开心\",\n        },\n        {\n          id: 3,\n          userId: 1,\n          content: \"今天上了acwing真开心\",\n        },\n      ],\n    });\n\n    const follow = () => {\n      if (user.is_followed) return;\n      user.is_followed = true;\n      user.followerCount++;\n    };\n\n    const unfollow = () => {\n      if (!user.is_followed) return;\n      user.is_followed = false;\n      user.followerCount--;\n    };\n\n    return {\n      user,\n      follow,\n      unfollow,\n      posts,\n    };\n  },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAcA,OAAOA,WAAU,MAAO,+BAA+B;AACvD,OAAOC,eAAc,MAAO,mCAAmC;AAC/D,OAAOC,gBAAe,MAAO,oCAAoC;AACjE,OAAOC,gBAAe,MAAO,mCAAmC;AAChE,SAASC,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVN,WAAW;IACXC,eAAe;IACfC,gBAAgB;IAChBC;EACF,CAAC;EAEDI,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIJ,QAAQ,CAAC;MACpBK,EAAE,EAAE,CAAC;MACLC,QAAQ,EAAE,OAAO;MACjBC,QAAQ,EAAE,KAAK;MACfC,SAAS,EAAE,IAAI;MACfC,aAAa,EAAE,CAAC;MAChBC,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAIX,QAAQ,CAAC;MACrBY,KAAK,EAAE,CAAC;MACRD,KAA