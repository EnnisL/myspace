{"ast":null,"code":"import \"core-js/modules/es.array.unshift.js\";\nimport ContentBase from \"../components/ContentBase\";\nimport UserProfileInfo from \"../components/UserProfileInfo\";\nimport UserProfilePosts from \"../components/UserProfilePosts\";\nimport UserProfileWrite from \"../components/UserProfileWrite\";\nimport { reactive } from \"vue\";\nexport default {\n  name: \"UserProfileView\",\n  components: {\n    ContentBase,\n    UserProfileInfo,\n    UserProfilePosts,\n    UserProfileWrite\n  },\n  setup() {\n    const user = reactive({\n      id: 1,\n      username: \"liule\",\n      lastName: \"Liu\",\n      firstName: \"Le\",\n      followerCount: 0,\n      is_followed: false\n    });\n    const posts = reactive({\n      count: 3,\n      posts: [{\n        id: 1,\n        userId: 1,\n        content: \"今天上了web课真开心\"\n      }, {\n        id: 2,\n        userId: 1,\n        content: \"今天上了算法课，更开心了\"\n      }, {\n        id: 3,\n        userId: 1,\n        content: \"今天上了acwing ，开心极了\"\n      }]\n    });\n    const follow = () => {\n      if (user.is_followed) return;\n      user.is_followed = true;\n      user.followerCount++;\n    };\n    const unfollow = () => {\n      if (!user.is_followed) return;\n      user.is_followed = false;\n      user.followerCount--;\n    };\n    const post_a_post = content => {\n      posts.count++;\n      posts.posts.unshift({\n        id: posts.count,\n        userId: 1,\n        content: content\n      });\n    };\n    return {\n      user,\n      follow,\n      unfollow,\n      posts,\n      post_a_post\n    };\n  }\n};","map":{"version":3,"names":["ContentBase","UserProfileInfo","UserProfilePosts","UserProfileWrite","reactive","name","components","setup","user","id","username","lastName","firstName","followerCount","is_followed","posts","count","userId","content","follow","unfollow","post_a_post","unshift"],"sources":["/Users/le/myspace/src/views/UserProfileView.vue"],"sourcesContent":["<template>\n  <ContentBase>\n    <div class=\"row\">\n      <div class=\"col-3\">\n        <UserProfileInfo @follow=\"follow\" @unfollow=\"unfollow\" :user=\"user\" />\n        <UserProfileWrite @post_a_post=\"post_a_post\" />\n      </div>\n      <div class=\"col-9\">\n        <UserProfilePosts :posts=\"posts\" />\n      </div>\n    </div>\n  </ContentBase>\n</template>\n\n<script>\nimport ContentBase from \"../components/ContentBase\";\nimport UserProfileInfo from \"../components/UserProfileInfo\";\nimport UserProfilePosts from \"../components/UserProfilePosts\";\nimport UserProfileWrite from \"../components/UserProfileWrite\";\nimport { reactive } from \"vue\";\n\nexport default {\n  name: \"UserProfileView\",\n  components: {\n    ContentBase,\n    UserProfileInfo,\n    UserProfilePosts,\n    UserProfileWrite,\n  },\n  setup() {\n    const user = reactive({\n      id: 1,\n      username: \"liule\",\n      lastName: \"Liu\",\n      firstName: \"Le\",\n      followerCount: 0,\n      is_followed: false,\n    });\n\n    const posts = reactive({\n      count: 3,\n      posts: [\n        {\n          id: 1,\n          userId: 1,\n          content: \"今天上了web课真开心\",\n        },\n        {\n          id: 2,\n          userId: 1,\n          content: \"今天上了算法课，更开心了\",\n        },\n        {\n          id: 3,\n          userId: 1,\n          content: \"今天上了acwing ，开心极了\",\n        },\n      ],\n    });\n\n    const follow = () => {\n      if (user.is_followed) return;\n      user.is_followed = true;\n      user.followerCount++;\n    };\n\n    const unfollow = () => {\n      if (!user.is_followed) return;\n      user.is_followed = false;\n      user.followerCount--;\n    };\n\n    const post_a_post = (content) => {\n      posts.count++;\n\n      posts.posts.unshift({\n        id: posts.count,\n        userId: 1,\n        content: content,\n      });\n    };\n\n    return {\n      user,\n      follow,\n      unfollow,\n      posts,\n      post_a_post,\n    };\n  },\n};\n</script>\n\n<style scoped></style>\n"